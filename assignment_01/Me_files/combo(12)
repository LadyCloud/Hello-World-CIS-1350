YUI.add("mail-common-newscookie",function(e){function t(t){var n=(new Date).getTime()+72e5;n=new Date(n),e.Cookie.set("YM.BREAKING_NEWS",t.uuid+"&"+t.publishedAt,{path:"/",domain:"mail.yahoo.com",expires:n})}function n(){if(e.Cookie.get("YM.BREAKING_NEWS")!==null)return e.Cookie.get("YM.BREAKING_NEWS")}e.namespace("mail.common"),e.namespace("mail.common.NewsCookie"),e.mix(e.mail.common.NewsCookie,{setBNCookie:t,getBNCookie:n})},"1.0.0",{requires:["node","cookie"]});
